    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@edx/frontend-build": "5.6.14",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "codecov": "3.8.3",
    "glob": "7.2.0",
    "husky": "6.0.0",
