{
  "name": "@edx/frontend-app-enterprise-public-catalog",
  "version": "1.0.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-enterprise-public-catalog.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --ext .js --ext .jsx . --fix",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-enterprise-public-catalog#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-enterprise-public-catalog/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.1.0",
    "@edx/brand-openedx": "^1.1.0",
    "@edx/frontend-component-footer": "10.2.2",
    "@edx/frontend-component-header": "2.4.6",
    "@edx/frontend-enterprise-catalog-search": "2.2.5",
    "@edx/frontend-platform": "1.15.6",
    "@edx/paragon": "^19.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "algoliasearch": "4.11.0",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "core-js": "3.18.1",
    "file-saver": "^2.0.5",
    "history": "5.1.0",
    "lodash.debounce": "4.0.8",
    "lodash.escaperegexp": "4.1.2",
    "prop-types": "15.7.2",
    "query-string": "7.0.1",
    "react": "16.12.0",
